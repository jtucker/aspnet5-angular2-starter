{
  "name": "aspnet5-angular2-starter",
  "version": "0.1.0",
  "author": "Jason Tucker",
  "description": "This is a starter template for getting going on ASP.Net 5 and Angular 2",
  "repository": {
    "type": "github",
    "url": "https://github.com/jtucker/aspnet5-angular2-starter"
  },
  "readme": "README.md",
  "license": "",
  "scripts": {
    "run": "dnx web ASPNET_ENV=Development",
    "postinstall": "jspm install && cd wwwroot && typings install"
  },
  "jspm": {
    "directories": {
      "baseURL": "wwwroot"
    },
    "dependencies": {
      "angular2": "npm:angular2@^2.0.0-beta.8",
      "bootstrap": "github:twbs/bootstrap@^3.3.6",
      "css": "github:systemjs/plugin-css@^0.1.20",
      "es6-shim": "github:es-shims/es6-shim@^0.35.0",
      "reflect-metadata": "npm:reflect-metadata@^0.1.3",
      "rxjs": "npm:rxjs@^5.0.0-beta.2",
      "ts": "github:frankwallis/plugin-typescript@^4.0.2",
      "zone.js": "npm:zone.js@^0.6.2"
    },
    "devDependencies": {
      "typescript": "npm:typescript@^1.6.2"
    }
  },
  "devDependencies": {
    "angular2": "^2.0.0-beta.8",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.3",
    "reflect-metadata": "^0.1.2",
    "rxjs": "^5.0.0-beta.2",
    "zone.js": "^0.5.15",
    "typescript": "1.7.5",
    "jspm": "^0.16.30",
    "gulp": "^3.9.1",
    "gulp-typescript": "^2.12.0",
    "gulp-sourcemaps": "^1.6.0",
    "typings": "^0.6.8",
    "concurrently": "^2.0.0"
  }
}
